integer binarySearch (integer left, integer right, integer target) {
    while (left <= right) {
        integer mid;
        mid = left + (right - left) DIV 2;
        if (mid == target) {
            return mid;
        }
        if (mid < target) {
            left = mid + 1;
        } else if(1==1) {
            right = mid - 1;
        }
    }
    return -1;
}

void Main() {
    integer left = 1;
    integer right = 10;
    integer target = 7;
    integer result;
    result = binarySearch(left, right, target);
    if (result == -1) {
        printf("Elemento no encontrado\n");
    } else {
        printf("Elemento encontrado: %d\n", result);
    }
}